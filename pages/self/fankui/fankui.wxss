/* pages/self/fankui/fankui.wxss */
.myOrder,.scan,.comproblem{
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 45px;
  width: 100%;
  height: 45px;
  background-color: white;
  border-bottom:1px solid #f5f5f5;
}
.myOrder image,.scan image,.comproblem image{
  margin-right: 16px;
  width: 20px;
  height: 20px;
}
.myOrder text,.scan text,.comproblem text{
  font-size: 14px;
  margin-left: 16px;
}